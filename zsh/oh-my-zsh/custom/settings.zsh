################### History ################### 
HISTFILE=~/.histfile    # File to save history in
HISTSIZE=1000
SAVEHIST=1000
setopt appendhistory          # MUltiple sessions writes to same file instead of overwriting
setopt hist_ignore_all_dups   # Ignores duplicate entries in history

################### Navigation ###################
setopt autocd   # Cd juste by typing directory name
